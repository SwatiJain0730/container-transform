{
    "id": "basic-3",
    "cmd": "python3 -m http.server 8080",
    "cpus": 1,
    "mem": 32,
    "networks": [ { "mode": "container/bridge" } ],
    "container": {
      "type": "DOCKER",
      "docker": {
        "image": "python:3"
      },
      "portMappings": [
        { "containerPort": 8080, "hostPort": 0 }
      ]
    }
  }